<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.com.healthhack.timedilation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.IntervalInstructionsActivity"
            android:label="Instructions"/>
        <activity android:name=".activity.IntervalBasicActivity"
            android:label="Level 1"/>
        <activity android:name=".activity.IntervalHistoryActivity"
            android:label="History"/>
        <activity android:name=".activity.IntervalSimultActivity"
            android:label="Level 2"/>
        <activity android:name=".activity.IntervalDistractorActivity"
            android:label="Level 3" />
    </application>

</manifest>